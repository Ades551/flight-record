{
    "compilerOptions": {
      "jsx": "react-jsx",
      "rootDir": "src",
      "module": "ES6",
      "target": "ES6",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    },
    "include": [
      "./src",
    ],
    "exclude": [
      "node_modules",
      "build"
    ]
}
